<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:GameStore_EF_MVVM.ViewModels"
                    xmlns:view="clr-namespace:GameStore_EF_MVVM.Views">


    <DataTemplate DataType="{x:Type vm:GamesViewModel}">
        <view:GamesView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:BuyersViewModel}">
        <view:BuyersView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StatisticViewModel}">
        <view:StatisticView DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>